&i2c1 {
    status = "okay";
    //clock-frequency = <100000>;
    bus-frequency = <100000>;
};

&smbus1 {
    status = "okay";
    
    mp5023: mp5023@40 {
        compatible = "mps,mp5023";
        reg = <0x40>;
        status = "okay";
        smbus-max-frequency = <100000>;
    };
};