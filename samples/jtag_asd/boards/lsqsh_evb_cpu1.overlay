&pinctrl{
    /omit-if-no-ref/mjtag1_gpio_pn10: mjtag1_gpio_pn10 {
        pinmux = <PN10>;
        gpio;
        output-enable;
    };
    /omit-if-no-ref/mjtag3_gpio_pf02: mjtag3_gpio_pf02 {
        pinmux = <PF02>;
        gpio;
        output-enable;
    };
};

&jtag1 {
    pinctrl-0 = <&mjtag1_tck_pn06 &mjtag1_tms_pn07 &mjtag1_tdi_pn09 &mjtag1_tdo_pn08 &mjtag1_trst_pn10>;
    pinctrl-1 = <&mjtag1_gpio_pn06 &mjtag1_gpio_pn07 &mjtag1_gpio_pn09 &mjtag1_gpio_pn10>;
    tck-gpios = <&gpion 6 0>;
    tms-gpios = <&gpion 7 0>;
    tdi-gpios = <&gpion 9 0>;
    tdo-gpios = <&gpion 8 0>;
    trst-gpios = <&gpion 10 0>;
    pinctrl-names = "default", "priv_start";
    label = "jtag1";
    status = "okay";
};

&jtag3 {
    pinctrl-0 = <&mjtag3_tck_pf04 &mjtag3_tms_pf08 &mjtag3_tdi_pf06 &mjtag3_tdo_pf10>;
    pinctrl-1 = <&mjtag3_gpio_pf04 &mjtag3_gpio_pf08 &mjtag3_gpio_pf06>;
    tck-gpios = <&gpiof 4 0>;
    tms-gpios = <&gpiof 8 0>;
    tdi-gpios = <&gpiof 6 0>;
    tdo-gpios = <&gpiof 10 0>;
    trst-gpios = <&gpiof 2 0>;
    pinctrl-names = "default", "priv_start";
    label = "jtag3";
    status = "okay";
};

