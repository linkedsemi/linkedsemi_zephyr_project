&zephyr_udc1 {
	 hid_dev_0 {
		compatible = "zephyr,hid-device";
		interface-name = "HID0";
		protocol-code = "none";
		in-polling-period-us = <1000>;
		in-report-size = <32>;
		out-polling-period-us = <1000>;
		out-report-size = <16>;
	};
	cdc_acm_uart0 {
		compatible = "zephyr,cdc-acm-uart";
	};
};
